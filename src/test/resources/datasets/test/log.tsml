<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="log">
    
    <adapter class="latis.reader.tsml.RegexAdapter"
             regex="\[(\S+)\s+(\w+)\s+(\S+)\s+\((\S+)\)\] (.*)"
             location="${log.file}" />
             
    <time name="time">
      <metadata units="yyyy-MM-dd'T'HH:mm:ss.SSS" />
    </time>
    
    <text name="level">
      <metadata length="5" />
    </text>
    
    <text name="class">
      <metadata length="32" />
    </text>
    
    <text name="thread">
      <metadata length="32" />
    </text>
    
    <text name="message">
      <metadata length="256" />
    </text>
  
  </dataset>

</tsml>
