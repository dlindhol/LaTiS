<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="composite_lyman_alpha" long_name="Composite Solar Lyman-alpha">
    <adapter class="latis.reader.tsml.IterativeAsciiAdapter"
             delimiter="\s+"
             commentCharacter=";"
             location="/lisird/public/lasp/lyman_alpha/composite_lya.dat" />
    
    <time name="time" type="text" length="7" units="yyyyDDD"/>
   
    <real name="LymanAlpha" long_name="Lyman-alpha irradiance" units ="1e11 photons/cm^2/sec"/>
    <integer  name="type" long_name="Source of the data" missing_value = "-999999"/>
    
  </dataset>

</tsml>
