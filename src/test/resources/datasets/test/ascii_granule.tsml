<?xml version="1.0" encoding="UTF-8"?>

<dataset id="ascii_granule">
  <adapter class="latis.reader.tsml.GranuleAsciiAdapter"
           delimiter="\s+"
           commentCharacter="#"
           location="datasets/test/mixed.txt" />
  
  <time type="text" length="10">
    <metadata name="myTime" units="yyyy/MM/dd"/>
  </time>
 
  <integer id="myInt"/>
  <real    id="myReal"/>
  <text    id="myText"/>
  
</dataset>
