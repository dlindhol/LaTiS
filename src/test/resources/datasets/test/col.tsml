<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset>
    <metadata name="Columnar Test"/>
    <adapter class="latis.reader.tsml.ColumnarAdapter"
             columns="0,1,2;5;3;4"
             delimiter=" "
             commentCharacter="#"
             location="datasets/test/col.txt" />
    
    <time name="time" units="yyyy MM dd"/> <!--Note, same delimiter as defined above-->
    
    <text name="text"/>
    <integer name="int"/>
    <real name="real"/>
    
  </dataset>

</tsml>
