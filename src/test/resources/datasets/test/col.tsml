<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset>
    <metadata name="Columnar Test"/>
    <adapter class="latis.reader.tsml.ColumnarAdapter"
             columns="0,1,2;5;3;4"
             delimiter="\s+"
             commentCharacter="#"
             location="datasets/test/col.txt" />
    
    <time name="time" type="text" length="10" units="yyyy MM dd"/> <!--Note, columns will be concat'd with space-->
    
    <integer name="int"/>
    <real name="real"/>
    <text name="text"/>
    
  </dataset>

</tsml>
