<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset>
    <metadata name="Columnar Test"/>
    <adapter class="latis.reader.tsml.ColumnarAdapter"
             columns="0,1,2;5;3;4"
             delimiter="\s+"
             commentCharacter="#"
             location="datasets/test/col.txt" />
    
    <time name="myTime" type="text" length="10" units="yyyy MM dd"/> <!--Note, columns will be concat'd with space-->
   
    <integer name="myInt"/>
    <real    name="myReal"/>
    <text    name="myText"/>
    
  </dataset>

</tsml>
