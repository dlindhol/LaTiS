<?xml version="1.0" encoding="UTF-8"?>

<dataset id="regex">
  <adapter class="latis.reader.tsml.RegexAdapter"
           pattern="(\S+)\s+(\S+)\s+(\S+)\s+(\S+)"
           commentCharacter="#"
           location="datasets/test/mixed.txt" />
  
  <time type="text">
    <metadata name="myTime" units="yyyy/MM/dd"/>
  </time>
 
  <integer id="myInt"/>
  <real    id="myReal"/>
  <text    id="myText"/>

</dataset>
