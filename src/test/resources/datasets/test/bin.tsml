<?xml version="1.0" encoding="UTF-8"?>

<dataset id="bin"> 

  <adapter class="latis.reader.tsml.BinaryAdapter"
           delimiter="/s+"
           location="datasets/data/tsi/bin" />
             
  <function length="403">
    <time>
      <metadata name="year"
                units="years since 0000-01-01" 
                long_name="Year"/>
    </time>
  
    <real>
      <metadata name="tsi"
                units="W/m^2"
                long_name="Total Solar Irradiance"/>
    </real>
  </function>

</dataset>
