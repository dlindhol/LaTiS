<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="international_sunspot_number" long_name="International Sunspot Number - Daily">
    <adapter class="latis.reader.tsml.ColumnarAdapter"
    		 columns="0,1,2;4"
             delimiter="\s+"
             commentCharacter="#"
             location="http://sidc.oma.be/DATA/dayssn_import.dat" />
    
    <time name="time" type="text" length="10" units="yyyy MM dd"/>
   
    <real name="ssn" long_name="Sunspot Number"/>
    
  </dataset>

</tsml>
