<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="tiff">

    <adapter class="latis.reader.tsml.GeoTiffAdapter"
             location="datasets/middle_tif.tif" />
             
    <function crs="EPSG:4326"
              width="1336"
              height="1598">
      <tuple>
        <real name="lat"/>
        <real name="lon"/>
      </tuple>
      <tuple>
        <integer name="red"/>
        <integer name="green"/>
        <integer name="blue"/>
      </tuple>
    </function>

  </dataset>

</tsml>
