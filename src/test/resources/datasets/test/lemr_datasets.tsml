<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="lemr_datasets">
    <adapter class="latis.reader.tsml.SparqlAdapter"
             location="http://lemr-dev:3030/VIVO/sparql" />
             
    <function>
      <index/>
      <text name="subject" length="256"/>
      <text name="predicate" length="256"/>
      <text name="object" length="256"/>
    </function>
    
    <?select query=PREFIX dcat:<http://www.w3.org/ns/dcat#> PREFIX dcterms:<http://purl.org/dc/terms/> SELECT ?s?p?o WHERE{?s a dcat:Dataset.?s?p?o} ?>
   
  </dataset>

</tsml>
