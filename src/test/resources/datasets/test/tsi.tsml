<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="tsi">

    <adapter class="latis.reader.tsml.IterativeAsciiAdapter"
    		 delimiter="\s+"
             location="datasets/tsi.txt" />
             
    <time name="year" 
          units="years since 0000-01-01" 
          long_name="Year"/>
    
    <real name="tsi"
          units="W/m^2"
          long_name="Total Solar Irradiance"/>

  </dataset>

</tsml>
