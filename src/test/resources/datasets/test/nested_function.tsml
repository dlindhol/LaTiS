<?xml version="1.0" encoding="UTF-8"?>

<dataset id="nested_function">
  <adapter class="latis.reader.tsml.AsciiAdapter"
           delimiter="\s+"
           location="datasets/test/nested_function.txt" />
  
  <time type="text">
    <metadata name="myTime" units="yyyy/MM/dd"/>
  </time>
 
  <function length="3">
    <integer id="myInt"/>
    <real    id="myReal"/>
    <text    id="myText"/>
  </function>
  
</dataset>
