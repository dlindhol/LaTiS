<?xml version="1.0" encoding="UTF-8"?>

<dataset id="two_time">
  <adapter class="latis.reader.tsml.AsciiAdapter"
		   delimiter="\s+"
           location="datasets/tsi.txt" />
             
  <time> 
    <metadata name="year"
              units="years since 0000-01-01" 
              long_name="Year"/>
  </time>
  
  <time>
    <metadata name="tsi"
              units="W/m^2"
              long_name="Total Solar Irradiance"/>
  </time>
  
</dataset>
