<?xml version="1.0" encoding="UTF-8"?>

<dataset id="maven_ldap">
  <adapter class="latis.reader.tsml.LdapAdapter"
           location="ldap://webiam-core1/dc=lasp,dc=colorado,dc=edu" 
           group="cn=maven-sdc-web-users,ou=groups,dc=lasp,dc=colorado,dc=edu"/>
        
  <!--
  <index/>
  <text name="uid" length="16"/>
  <text name="cn" length="32"/>
  <text name="mail" length="64"/>
  <text name="memberOf" length="2048"/>
  -->
           
  <function id="people">
    <text id="uid" length="16"/>
    <tuple id="person">
      <text id="cn" length="32"/>
      <text id="mail" length="64"/>
      <text id="memberOf" length="2048"/>
    </tuple>
  </function>
  
</dataset>
