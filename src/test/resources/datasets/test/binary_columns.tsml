<?xml version="1.0" encoding="UTF-8"?>

<dataset id="binary_columns">
  <adapter class="latis.reader.tsml.ColumnarBinaryAdapter"
           location="${java.io.tmpdir}" />
  
  <time type="text" length="10">
    <metadata name="myTime" units="yyyy/MM/dd"/>
  </time> 
 
  <integer id="myInt"/>
  <real    id="myReal"/>
  <text    id="myText"/>
  
</dataset>
