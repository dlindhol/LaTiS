<?xml version="1.0" encoding="UTF-8"?>

<dataset>
  <metadata name="data_with_marker"/>
  <adapter class="latis.reader.tsml.AsciiAdapter"
           delimiter="\s+"
           marker="DATA"
           location="datasets/test/data_with_marker.txt" />
           
  <time>
    <metadata name="year" 
              units="years since 0000-01-01" 
              long_name="Year"/>
  </time>
  
  <real>
    <metadata name="tsi"
              units="W/m^2"
              long_name="Total Solar Irradiance"/>
  </real>

</dataset>
