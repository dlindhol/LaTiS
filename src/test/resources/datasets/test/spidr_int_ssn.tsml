<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="spidr_int_ssn" long_name="SPIDR International Sunspot Number">
    <adapter class="latis.reader.tsml.ColumnarAdapter"
             columns="0;1"
             delimiter=","
             commentCharacter="#"
             location="http://spidr.ngdc.noaa.gov/spidr/servlet/GetData?format=csv%26dateFrom=19320101%26dateTo=21000101%26param=index_ssn"/>
    
    <time name="time" type="text" length="16" units="yyyy-MM-dd HH:mm"/>
   
    <real name="ssn" long_name="International Sunspot Number" units="count" missing_value="1e33"/>
    
  </dataset>

</tsml>
