<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="files">
    
    <adapter class="latis.reader.tsml.FileListAdapter"
             pattern="Foo(\d{7})bar(\d)v(\d.\d)(\w).dat"
             location="/tmp/latis_file_test"
             todo_location="${java.io.tmpdir}/latis_file_test" />
             
    <time name="myTime" units="yyyyDDD" type="text" length="7"/>
   
    <integer name="myInt"/>
    <real    name="myReal"/>
    <text    name="myText"/>
    
    <text name="file" length="256"/>
  
  </dataset>

</tsml>
