<?xml version="1.0" encoding="UTF-8"?>

<dataset id="files">
  
  <adapter class="latis.reader.tsml.FileListAdapterNio"
           pattern="spacecraft(\d+)_instrument(\d+)_mode(\d+)_day(\d+).txt"
           location="/tmp/latis_file_test_large_flat" />
           
  <index/>
  <integer id="spacecraftNum"/>
  <integer id="instrumentNum"/>
  <integer id="modeNum"/>
  <integer id="dayNum"/>
  
  <text id="file" length="256"/>
  <integer id="fileSize"/>

</dataset>
