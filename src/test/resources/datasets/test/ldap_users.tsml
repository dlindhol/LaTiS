<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="maven_sdc_web_users">
    <adapter class="latis.reader.tsml.LdapAdapter"
             location="ldap://webiam-core1/dc=lasp,dc=colorado,dc=edu" />
             
      <function name="people">
        <text name="uid" length="16"/>
        <tuple name="person">
          <text name="cn" length="32"/>
          <text name="mail" length="64"/>
          <!--text name="memberOf" length="2048"/-->
          <sequence name="memberOf">
            <text name="group" regex="cn=(\w+)," length="32"/>
          </sequence>
        </tuple>
      </function>
    
  </dataset>

</tsml>


