<?xml version="1.0" encoding="UTF-8"?>

<dataset id="maven_sdc_web_users">
  <adapter class="latis.reader.tsml.LdapAdapter"
           location="ldap://webiam-core1/dc=lasp,dc=colorado,dc=edu" />
           
  <function id="people">
    <text id="uid" length="16"/>
    <tuple id="person">
      <text id="cn" length="32"/>
      <text id="mail" length="64"/>
      <!--text name="memberOf" length="2048"/-->
      <sequence id="memberOf">
        <text id="group" regex="cn=(\w+)," length="32"/>
      </sequence>
    </tuple>
  </function>
  
</dataset>



