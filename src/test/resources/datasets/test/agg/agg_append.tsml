<?xml version="1.0" encoding="UTF-8"?>
<dataset id="agg_test">
  <adapter class="latis.reader.tsml.agg.TileUnionAdapter"/>

  <dataset id="ds1">
    <adapter class="latis.reader.tsml.AsciiAdapter"
             delimiter="\s+"
             location="datasets/test/agg/canonical_union_1.txt" />
    <time type="text" length="10">
      <metadata name="myTime" units="yyyy/MM/dd"/>
    </time>
   
    <integer id="myInt"/>
    <real    id="myReal"/>
    <text    id="myText"/>
  </dataset>

  <dataset id="ds2">
    <adapter class="latis.reader.tsml.AsciiAdapter"
             delimiter="\s+"
             location="datasets/test/agg/canonical_union_2.txt" />
    <time type="text" length="10">
      <metadata name="myTime" units="yyyy/MM/dd"/>
    </time>
   
    <integer id="myInt"/>
    <real    id="myReal"/>
    <text    id="myText"/>
  </dataset>

</dataset>