<?xml version="1.0" encoding="UTF-8"?>

<dataset id="file_agg">
  <adapter class="latis.reader.tsml.FileAggAdapter"
           template="agg/scalar_ts_2col_0to9"
           pattern="scalar_ts_2col_1?0-(1?9).txt"
           location="datasets/test/agg" />
           
  <function>
    <time id="time" type="real" units="s"/>
