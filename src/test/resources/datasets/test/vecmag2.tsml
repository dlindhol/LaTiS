<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="vecmag">

    <adapter class="latis.reader.tsml.AsciiAdapter"
    		 delimiter="\s+"
             location="datasets/test/vecmag.txt" />
             
    <time name="t"/>
    
    <real name="a"/>
    <real name="b"/>
    <real name="c"/>
    <?derived X = sqrt(a*a+b*b+c*c)?>
    <?derived X2 = mag(a,b,c)?>
    <?derived Y = X + a?>
    <!--?derived Z = 123.4?-->
      

  </dataset>

</tsml>
