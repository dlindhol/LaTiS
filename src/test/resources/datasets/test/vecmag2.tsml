<?xml version="1.0" encoding="UTF-8"?>

<dataset id="vecmag">
  <adapter class="latis.reader.tsml.AsciiAdapter"
   		   delimiter="\s+"
           location="datasets/test/vecmag.txt" />
             
  <time id="t"/>
  
  <real id="a"/>
  <real id="b"/>
  <real id="c"/>
  <?derived X = SQRT(a*a+b*b+c*c)?>
  <?derived X2 = MAG(a,b,c)?>
  <?derived Y = X + a?>
  <?derived Z = 123.4?> 

</dataset>