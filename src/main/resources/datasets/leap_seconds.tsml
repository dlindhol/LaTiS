<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="leap_seconds">
    <adapter class="latis.reader.tsml.ColumnarAdapter"
             commentCharacter="#"
             delimiter="\s+"
             columns="0;1"
             location="http://www.ietf.org/timezones/data/leap-seconds.list" />
             
    <time type="integer" units="UTC seconds since 1900-01-01"/>
    <integer name="leap_seconds"/>

  </dataset>

</tsml>
