<?xml version="1.0" encoding="UTF-8"?>

<dataset>
  <metadata name="lemr_datasets"/> 
  <adapter class="latis.reader.tsml.SparqlAdapter"
           location="http://lemr-dev:3030/VIVO/sparql" 
           query="PREFIX dcat:&lt;http://www.w3.org/ns/dcat#&gt; PREFIX dcterms:&lt;http://purl.org/dc/terms/&gt; SELECT ?s?p?o WHERE{?s a dcat:Dataset.?s?p?o}"
           />
           <!-- Note, in the above query, "&lt;" means '<' and "&gt;" means '>' -->
           
  <function>
    <index/>
    <text length="256">
      <metadata name="subject"/>
    </text>
    <text length="256">
      <metadata name="predicate"/>
    </text>
    <text length="256">
      <metadata name="object"/>
    </text>
  </function>
 
</dataset>
