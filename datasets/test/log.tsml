<?xml version="1.0" encoding="UTF-8"?>
<tsml>

  <dataset name="log">
    
    <adapter class="latis.reader.tsml.RegexAdapter"
             regex="\[(.+)\] (.+)"
             url="/data/latis/log.txt" />
            <!-- url="${log.file}" /-->
             
    <time name="time">
      <metadata units="yyyy-MM-dd" />
    </time>
    
    <text name="message">
      <metadata length="80" />
    </text>
  
  </dataset>

</tsml>
